{
  "ConnectionStrings": {
    "sls": "Data Source=localhost;User ID=sa;Password=Testpass123;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:1111",
        "Protocols": "Http1"
      },
      "Grpc": {
        "Url": "http://*:1112",
        "Protocols": "Http2"
      }
    }
  },
  "nlog": {
    "internalLogFile": "file.txt",
    "targets": {
      "async": "true",
      "error_file": {
        "type": "File",
        "fileName": "${basedir}/logs/error_file.txt",
        "archiveFileName": "${basedir}/archives/log.{#}.txt",
        "archiveEvery": "Day",
        "archiveNumbering": "Rolling",
        "maxArchiveFiles": "7",
        "self-closing": "true"
      },
      "multi": {
        "type": "File",
        "fileName": "${basedir}/logs/${logger}.log",
        "archiveFileName": "${basedir}/archives/${logger}.{#}.log",
        "archiveEvery": "Day",
        "archiveNumbering": "Rolling",
        "maxArchiveFiles": "7",
        "self-closing": "true"
      }
    },
    "rules": [
      {
        "logger": "CarController",
        "minlevel": "Debug",
        "writeTo": "multi",
        "final": "true",
        "self-closing": "true"
      },
      {
        "logger": "CalculateController",
        "minlevel": "Debug",
        "writeTo": "multi",
        "final": "true",
        "self-closing": "true"
      },
      {
        "logger": "UserController",
        "minlevel": "Debug",
        "writeTo": "multi",
        "final": "true",
        "self-closing": "true"
      }
    ]
  }

}
